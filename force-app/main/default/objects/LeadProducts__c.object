    <validationRules>
        <fullName>Discount_Products_Must_Be_Backend_Create</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
    Is_Discount_Product__c = TRUE,
    Is_Backend_Created__c = FALSE
)</errorConditionFormula>
        <errorMessage>Discount products can only be created through the backend logic.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enforce_Max_Quantity_Per_Product</fullName>
        <active>false</active>
        <description>If a Product has a Max Qty Per Order, this validation rule blocks higher quantities of it.</description>
        <errorConditionFormula>AND(
    NOT(ISBLANK(Product__r.Maximum_Quantity_Per_Order__c)),
    Quantity__c &gt; Product__r.Maximum_Quantity_Per_Order__c
)</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>You can&apos;t order more than the allowed maximum quantity for this product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_change_owner</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR($Profile.Name = &quot;Sales Agent&quot;,$Profile.Name = &quot;Legal&quot;)
, ISCHANGED( OwnerId ) )</errorConditionFormula>
        <errorMessage>You don&apos;t have permission to change the record owner</errorMessage>
    </validationRules>
